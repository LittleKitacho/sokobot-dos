{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "npm",
			"args": ["run", "build"],
			"problemMatcher": [
				"$tsc",
				"$eslint-stylish"
			],
			"group": {
				"isDefault": true,
				"kind": "build"
			},
			"label": "build",
			"presentation": {
				"close": true,
				"reveal": "silent",
				"revealProblems": "onProblem"
			},
		},
	]
}